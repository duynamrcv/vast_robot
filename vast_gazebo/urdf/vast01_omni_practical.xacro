<?xml version='1.0'?>

<robot name="vast_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  	<xacro:include filename="$(find vast_gazebo)/urdf/vast1.gazebo" />
	<xacro:property name="scale" value="0.01" />

  	<link name="base_footprint"></link>

	<joint name="robot_footprint_joint" type="fixed">
		<origin xyz="0 0 0.07" rpy="0 0 0" />
		<parent link="base_footprint" />
		<child link="base_link" />
	</joint>

	<!-- Base Link -->
	<link name="base_link">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size="0.4 0.4 0.05"/>
				<!--long,wide,tall-->
			</geometry>
			
			<material name="blue">
				<color rgba="0 0.5 1 1"/>
			</material>
		</visual>
		<!-- Base collision -->
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size="0.4 0.4 0.05"/>
				<!--long,wide,tall-->
			</geometry>
		</collision>

		<inertial>
			<mass value="${10.0*scale}"/>
			<inertia ixx="0.5" ixy="0.0" ixz="0.0"
					iyy="0.1391" iyz="0.0" izz="1.6125"/>
		</inertial>
	

		<visual name="left_bottom_cylinder">
			<origin xyz="0.15 -0.15 0.25" rpy="0 0 0" />
			<geometry>
				<box size="0.01 0.01 0.5" />
			</geometry>
			<material name="white">
				<color rgba="0 0 0 1"/>
			</material>
		</visual>
		<!-- left bottom cylinder collision -->

		<!-- right_bottom cylinder -->
		<visual name="right_bottom_cylinder">
			<origin xyz="0.15 0.15 0.25" rpy="0 0 0" />
			<geometry>
				<box size="0.01 0.01 0.5" />
			</geometry>
			<material name="white"/> 
		</visual>

		<!-- between_top_cylinder -->
		<visual name="between_top_cylinder">
			<origin xyz="-0.15 0 0.25" rpy="0 0 0" />
			<geometry>
				<box size="0.01 0.01 0.5" />
			</geometry>
			<material name="white"/>
		</visual>

		<!-- hold_box1 -->
		<visual name="hold_box1">
